<% if (title) { %>
   <h1 class="account-management-title"><%= title %></h1>
<% } else {
   res.redirect('/')
} %>

<% if (message) { %>
   <p class="notice"><%= message %></p>
<% } %>

<% if (errors) { %>
   <ul class="notice">
   <% errors.array().forEach(error => { %>
      <li><%= error.msg %></li>
   <%  }) %>
   </ul>
<% } %>

<p>LOGIN SUCCESS SUCKERS</p>

